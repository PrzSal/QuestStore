<header>Add new class</header>
<div class="form-in">
    <form method="POST">
        <label>Class Name </label><br>
        <input type="text" name="className" placeholder="enter class name..." required> <br>
        <input type="submit" value="Submit">
    </form>
</div>
<div class="table">
    <table class="tg">
        <div class="text">Team Member</div>
        <td class="tg-gkax">Nr.</td>
        <td class="tg-gkax">Name </td>
        <td class="tg-gkax">Artifact </td>
        {% for shop in teamShopModels %}
            <tr>
                <td class="tg-gkax">{{ loop.index }}</td>
                <td class="tg-gkax">{{ shop.nameTeam }}</td>
                <td class="tg-gkax">{{ shop.teamId }} </td>
            </tr>
        {% endfor %}
    </table>
</div>